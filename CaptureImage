Sub GenerateImage()

    Worksheets("Dashboard").Activate
    Sheets("Dashboard").Select
    Range("A1").Select
    Set Capture = ActiveWorkbook.Worksheets("Dashboard").Range("A1:R120")  
    
    Capture.CopyPicture
    With ActiveWorkbook.Worksheets("Dashboard").ChartObjects.Add(Capture.Left, Capture.Top, Capture.Width, Capture.Height)
        .Activate
        .Chart.Paste
        .Chart.Export "C:\Users\Glenn Dalida\Desktop\intraday\intraday.PNG"
    End With
    Worksheets("Dashboard").ChartObjects(Worksheets("Dashboard").ChartObjects.Count).Delete
    Set Capture = Nothing
    
    Sheets("Dashboard").Select
    Range("A1").Select
    
End Sub
